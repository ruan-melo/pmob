{
  "expo": {
    "name": "projeto-final",
    "slug": "projeto-final",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/icon.png",
    "splash": {
      "image": "./public/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ruantmelo.music.player"
    },
    "web": {
      "favicon": "./public/favicon.png"
    }
  }
}
